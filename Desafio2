package lpa2;
import java.util.Scanner;

public class Fifas {

	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		
		double Valor,Area;
		
		System.out.println("Informe o valor: ");
		Valor = ler.nextDouble();
		
		Area = Valor*Valor;
		
		System.out.println("A área desse objeto é: " +Area);

	}

}
