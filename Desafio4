package lpa2;

import java.util.Scanner;

public class BingoDoCR7 {

	public static void main(String[] args) {
    Scanner ler = new Scanner(System.in);
		
		int Bingo[] = new int [20];
		
		//Entrada de dados
		for(int i=0; i<20; i++) {
			Bingo[i] = (int)Math.round(Math.random()*100);
			}
		
        // Saída de dados
		for(int i=0; i<20; i++) {
				System.out.println(" Os valores são " + Bingo[i]);
			}
		
		ler.close();
	
	}

}
